{
  "manifest_version": 2,
  "name": "Translit",
  "version": "8",
  "description": "__MSG_description__",
  "default_locale": "en",
  "icons": {"16":"images/icon.png",
	        "48":"images/icon48.png"},
  "options_page": "html/options.html",

  "content_scripts": [ {
    "matches": [ "http://*/*","https://*/*" ],
